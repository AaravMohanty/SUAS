   "source": [
    "import socket\n",
    "\n",
    "client = socket.socket(socket.AF_INET, socket.SOCK_STREAM)\n",
    "client.connect(('localhost', 1002))\n",
    "\n",
    "file = open('IMG_1664.jpg', 'rb')\n",
    "image_data = file.read(2048)\n",
    "\n",
    "while image_data:\n",
    "    client.send(image_data)\n",
    "    image_data = file.read(2048)\n",
    "\n",
    "file.close()\n",
    "client.close()\n"
   ]
  }
